{
  "manifest_version": 3,
  "name": "KlimbB Extension",
  "version": "1.0.0",
  "description": "A Chrome Extension developed using React + TypeScript + Redux",
  "action": {
    "default_title": "KlimbB Extension",
    "default_popup": "popup.html"
  },
  "permissions": ["storage", "tabs", "activeTab", "declarativeContent"],
  "host_permissions": ["https://www.youtube.com/watch?v*"]
}
